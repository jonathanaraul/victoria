{%for object in objects%}
	{%if object.title|upper != 'SPACER'%}
	<div class="menu-element">
		<a href="{{path('proyecto_front_page',{'id': object.id, 'friendlyname' : object.friendlyName})}}">{{object.title|upper}}</a>
		{%if idpage == object.id%}
			<hr class="men-sel" style="background-color: #ffffff;">
		{%endif%}
	</div>
	{%else%}
	<div class="menu-element spacer"></div>
	{%endif%}
{%endfor%}